{
  "name": "vue-route-generator",
  "version": "1.0.0",
  "author": "katashin",
  "description": "Vue Router route config generator",
  "keywords": [
    "Vue",
    "Vue Router",
    "routing",
    "generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ktsn/vue-route-generator",
  "bugs": "https://github.com/ktsn/vue-route-generator/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vue-route-generator.git"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf lib",
    "build": "tsc -p src",
    "dev": "jest --watch",
    "lint": "tslint -p . && prettier --list-different \"{src,scripts,test}/**/*.{js,ts}\"",
    "format": "prettier --write \"{src,scripts,test}/**/*.{js,ts}\"",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "test/tsconfig.json"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "@types/prettier": "^2.1.6",
    "@vue/compiler-sfc": "^3.0.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-ktsn": "^2.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "vue": "^3.0.5",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "fast-glob": "^3.2.4",
    "prettier": "2.2.1"
  }
}
